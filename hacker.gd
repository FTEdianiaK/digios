func _process(delta):
	if Input.is_action_just_pressed("keyboard"):
		if $Root/Center/ScrollContainer/Console.visible_ratio == 1:
			$Root/Center/ScrollContainer/Console.visible_ratio = 0
			var rng = randi_range(0,2)
			$Root/Center/ScrollContainer/Console.text = codes[rng]
		else:
			$Root/Center/ScrollContainer/Console.visible_ratio += 0.001
	elif Input.is_action_just_pressed("ui_end"):
		if $Root/Center/ScrollContainer.is_visible_in_tree() == true:
			$Root/Center/ScrollContainer.hide()
			$Root/Center/State.show()
		else:
			$Root/Center/ScrollContainer.show()
			$Root/Center/State.hide()
			$Root/Center/StateEdit.hide()


func _on_decrypt_timer_timeout():
	cur["decP"] += randi_range(15,25)
	if cur["decP"] > 100:
		cur["decP"] = 0
		cur["decS"] += 1

	if cur["decS"] == 1:
		$Root/Left/Decrypt/Steps.text = "Loading file... " + str(cur["decP"]) + "%"
	elif cur["decS"] == 2:
		$Root/Left/Decrypt/Steps.text = "Loading file... 100%\nFinding header... " + str(cur["decP"]) + "%"
	elif cur["decS"] == 3:
		$Root/Left/Decrypt/Steps.text = "Loading file... 100%\nFinding header... 100%\nVerifying content... " + str(cur["decP"]) + "%"
	elif cur["decS"] == 4:
		$Root/Left/Decrypt/Steps.text = "Loading file... 100%\nFinding header... 100%\nVerifying content... 100%\nLoading codes... " + str(cur["decP"]) + "%"
	elif cur["decS"] == 5:
		$Root/Left/Decrypt/Steps.text = "Loading file... 100%\nFinding header... 100%\nVerifying content... 100%\nLoading codes... 100%"
		$Root/Left/Decrypt/Loading.text = "DONE"
		await get_tree().create_timer(2.0).timeout
		cur["decS"] = 1
		$Root/Left/Decrypt/Steps.text = "Loading file... " + str(cur["decP"]) + "%"
		$Root/Left/Decrypt/Loading.text = "Working..."

	$DecryptTimer.start(randf_range(1.5,2.5))


func _on_password_timer_timeout():
	cur["pass"] += 1
	if cur["pass"] == 10:
		cur["pass"] = 0

	$Root/Left/Password/List.text = "{password: '" + str(passList[cur["pass"]]) + "', salt: '" + str(Marshalls.utf8_to_base64(passList[cur["pass"]])) + "', hash: '" + str(passList[cur["pass"]].hash()) + "'}"
	$PasswordTimer.start(randf_range(0.5,1.5))


func _on_refresh_timer_timeout():
	for i in range(1,21):
		get_node("Root/Left/Matrix/Label" + str(i)).text = str(randi_range(0,1))

	$RefreshTimer.start(randf_range(1.0,3.0))
	cur["cpu"] = randi_range(2,9)
	cur["mem"] = randi_range(1,10)
	cur["net"] = randi_range(0,7)
	$Root/Center/Resources/Monitor/Bars.text = "[" + "|".repeat(cur["cpu"]) + " ]\n[" + "|".repeat(cur["mem"]) + " ]\n[" + "|".repeat(cur["net"]) + " ]"
	$Root/Center/Resources/Monitor/Percentage.text = str(int(cur["cpu"] / 0.1)) + "%\n" + str(int(cur["mem"] / 0.1)) + "%\n" + str(int(cur["net"] / 0.1)) + "%"


func _on_tasks_timer_timeout():
	cur["task"] += 1
	if cur["task"] > 4:
		$Root/Center/Resources/Tasks.lines_skipped = cur["task"] - 4

	$Root/Center/Resources/Tasks.text += "\n" + str(cur["task"])
	cur["tTimer"] = randf_range(0.5,1.5)
	$TasksTimer.start(cur["tTimer"])
	$Root/Right/Ping/Label.text += "\nResponse from " + cur["target"] + ": bytes=32 time=" + str(int(cur["tTimer"]*1000)) + "ms TTL=64"
	cur["ping"] += 1
	if cur["ping"] > 2:
		$Root/Right/Ping/Label.lines_skipped = 1 + 2*(cur["ping"]-2)


func _on_ip_timer_timeout():
	$Root/Right/Network/Color.color = "#009b9b"
	$Root/Right/Network/Status.text = "CONNECTION RESET"
	$TasksTimer.stop()
	await get_tree().create_timer(randf_range(0.5,1.5)).timeout
	var c = randi_range(1,10)
