		$Root/Right/Network/Status.text = "CONNECTION UNSTABLE"
	else:
		$Root/Right/Network/Color.color = "#009b00"
		$Root/Right/Network/Status.text = "CONNECTION STABLE"

	cur["target"] = "192.168." + str(randi_range(0,99)) + "." + str(randi_range(0,999))
	$Root/Right/IP/Label.text = cur["target"] + "\n192.168." + str(randi_range(0,99)) + "." + str(randi_range(0,999)) + "\n192.168." + str(randi_range(0,99)) + "." + str(randi_range(0,999))
	cur["ping"] = 0
	$Root/Right/Ping/Label.text = "Pinging " + cur["target"] + ":"
	$Root/Right/Ping/Label.lines_skipped = 0
	cur["tTimer"] = randf_range(0.5,1.5)
	$TasksTimer.start(cur["tTimer"])
	$IPTimer.start(randf_range(8.0,12.0))


